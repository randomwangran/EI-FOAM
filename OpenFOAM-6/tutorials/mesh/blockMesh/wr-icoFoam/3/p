FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "3";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform -9.82325e-15;

boundaryField
{
    frontAndBack
    {
        type            slip;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    lowerWall
    {
        type            zeroGradient;
    }
    upperWall
    {
        type            slip;
    }
}


